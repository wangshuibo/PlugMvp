<idea-plugin>
    <id>MisWangMvp.MvpCreateAction</id>
    <name>MisWangMvp</name>
    <version>1.0</version>
    <vendor email="203911607@qq.com" url="https://wangshuibo.github.io/">王水波</vendor>

    <description><![CDATA[
      Enter short description for your plugin here.<br>
      <em>most HTML tags may be used</em>
    ]]></description>

    <change-notes><![CDATA[
      Add change notes here.<br>
      <em>most HTML tags may be used</em>
    ]]>
